FLASK_APP=app.py #Variavel que recebe valor # onde o arquivo está a aplicação e oflask irá executar a aplicação
FLASK_ENV=development #Tipo de recurso que serve para ser usado em um ambiente de desenvolvimento
FLASK_HOST=0.0.0.0 # define o endereço do host(máquina) esperando que requisições sejam feitas para ele 
FLASK_DEBUG=1

# isso tudo serve para que não precise mudar o port toda vez que use flask run
